{
  "name": "fCinema",
  "productName": "fCinema",
  "version": "0.1.3",
  "description": "Decide what you watch. Unleash your personality",
  "homepage": "https://wwww.fcinema.org",
  "main": "main.js",
  "author": "fCinema Team",
  "license": "ISC",
  "scripts": {
    "package-mac": "electron-packager . fCinema --overwrite --asar --platform=darwin --arch=x64 --icon=icons/icon.icns --prune=true --out=release-builds --osx-sign",
    "package-win": "electron-packager . fCinema --overwrite --asar --platform=win32 --arch=ia32 --icon=icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=fCinema --version-string.FileDescription=0.1.1 --version-string.ProductName=\"fCinema\"",
    "package-linux": "electron-packager . fCinema --overwrite --asar --platform=linux --arch=x64 --icon=icons/icon.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/fCinema-darwin-x64/fCinema.app fCinema --out=release-builds --overwrite --icon=icons/icon.icns",
    "create-installer-win": "node installers/windows.js",
    "start": "electron .",
    "bower": "bower install"
  },
  "devDependencies": {
    "bower": "^1.8.2",
    "electron": "^1.8.1",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^10.1.0",
    "electron-winstaller": "^2.6.3"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.1",
    "angular-three-state-checkbox": "^1.1.0",
    "electron-is-dev": "^0.3.0",
    "electron-widevinecdm": "^1.0.2",
    "express": "^4.14.0",
    "file-bytes": "^2.0.3",
    "fs": "0.0.1-security",
    "get-pixels": "^3.3.0",
    "node-persist": "^2.0.7",
    "opensubtitles": "0.0.4",
    "request": "^2.75.0",
    "tmp": "0.0.29",
    "which": "^1.2.11"
  },
  "postinstall": "install-app-deps"
}
